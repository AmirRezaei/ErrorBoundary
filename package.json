  "scripts": {
    "setup": "yarn install",
    "install:yarn": "npm install -g yarn || echo 'Yarn is already installed'",
    "dev": "vite dev --port 3000",
    "dev:debug": "npx clear-cli && cross-env DEBUG=vite:* yarn dev",
    "start": "yarn dev",
    "UpgradeToLatest": "npx clear-cli && yarn upgrade --latest && yarn deduplicate && yarn install && yarn clean && yarn dev",
    "preview": "vite preview",
    "lint": "npx clear-cli && eslint .",
    "lint:fix": "npx clear-cli && eslint . --fix",
    "format": "npx clear-cli && prettier --write .",
    "format:eslint": "npx clear-cli && eslint . --fix && prettier --write .",
    "format:debug": "cross-env DEBUG=true prettier --write .",
    "clean": "npx rimraf dist && npx rimraf node_modules/.vite && npx clear-cli",
    "clean:all": "npx rimraf node_modules && yarn",
    "clean:prettier": "yarn prettier --write . --cache --cache-strategy content",
    "analyze": "vite build --mode analyze",
    "check:types": "tsc --noEmit",
    "prepend-header": "node prepend-filename.js",
    "prepend-client-directive": "node prepend-client-directive.js",
    "deploy": "yarn build && npx cpy-cli \"dist/**/*\" \"W:/\" --recursive",
    "build": "npx clear-cli && npx rimraf dist && yarn check:types && vite build",
    "reinstall": "npx rimraf node_modules && npx rimraf yarn.lock && yarn cache clean && yarn install && yarn deduplicate",
    "deduplicate": "yarn-deduplicate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "yarn audit",
    "test:ci": "jest --ci --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
